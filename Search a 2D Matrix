  bool searchMatrix(vector<vector<int>>& v, int target) {
        for(int i=0;i<v.size();i++){
            if(*(v[i].begin()) <= target && v[i].back() >= target){
                if(find(v[i].begin(),v[i].end(),target) != v[i].end())
                return true;
            }
        }
        return false;
    }
