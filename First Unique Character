    int firstUniqChar(string s) {
        
        unordered_map<int,int> m;
        for(int i = 0;i<s.length();i++){
            if(m.find(s[i]) != m.end()){
                m[s[i]] = -1;
            }
            else{
                m.insert({s[i],i});
            }
            
        }
        
        for(int i = 0;i<s.length();i++){
            if(m[s[i]] != -1)
                return i;
        }        
        return -1;
    }
