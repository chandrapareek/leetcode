  
   bool searchMatrix(vector<vector<int>>& v, int target) {
        int rows = v.size(),row = 0,col = v[0].size()-1;
        while(row < rows && col > -1){
            int cur = v[row][col];
            if(cur == target) return true;
            if(cur < target) row++;
            else col--;
        }
        
        return false;
    }
    
